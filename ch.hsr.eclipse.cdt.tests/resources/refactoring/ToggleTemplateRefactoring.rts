//@.config
filename=A.h
//@A.h
#include <iostream>

template <typename T, typename U>
class A {
  class B {
    T /*$*/member/*$$*/()
    {
        return T();
    }
  };
};
//=
#include <iostream>

template <typename T, typename U>
class A {
  class B {
    T member();
  };
};

template<typename T, typename U> inline T A<T,U>::B::member()
{
    return T();
}


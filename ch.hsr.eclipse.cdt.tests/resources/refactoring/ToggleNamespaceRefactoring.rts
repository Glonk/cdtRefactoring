//!TestSimpleNamespaceInClassToInHeader
//#ch.hsr.eclipse.cdt.ui.tests.toggle.ToggleRefactoringTest
//@.config
filename=A.h
//@A.h
#include <iostream>
#include <exception>

namespace N {
    class A {
        void /*$*/foo/*$$*/() {
            return;
        }
    };
}
//=
#include <iostream>
#include <exception>

namespace N {
    class A {
        void foo();
    };
    inline void A::foo()
    {
        return;
    }

}

//!TestInsideBodyAndSameFileComment
//#ch.hsr.eclipse.cdt.ui.tests.toggle.ToggleRefactoringTest
//@.config
filename=A.h
//@A.h
class A {
    void /*$*/member/*$$*/()
    {
        // a declaration statement
        int foo();
        // an expression statement
        int i = 3;
        // and a return statement
        return;
    }
};
//=
class A {
    void member();
};

inline void A::member()
{
    // a declaration statement
    int foo();
    // an expression statement
    int i = 3;
    // and a return statement
    return;
}


